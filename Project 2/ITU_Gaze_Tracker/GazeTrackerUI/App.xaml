<Application x:Class="GazeTrackerUI.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    StartupUri="GazeTrackerUIMainWindow.xaml">
    <Application.Resources>
         
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    		<ResourceDictionary Source="/Style/Simple Styles.xaml"/>
                <ResourceDictionary x:Name="IphoneStyle" Source="/Style/style_iphone.xaml" />
                <ResourceDictionary x:Name="IphoneStyleSmall" Source="Style/style_iphonesmall.xaml"/>
                <ResourceDictionary x:Name="DataGridStyle" Source="/Style/style_datagrid.xaml" />
                <ResourceDictionary x:Name="TabControlStyle" Source="/Style/style_datagrid.xaml" />
                <ResourceDictionary x:Name="EditSlider" Source="/Style/style_editslider.xaml"/>
             </ResourceDictionary.MergedDictionaries>
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
